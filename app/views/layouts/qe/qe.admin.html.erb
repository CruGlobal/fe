<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.1//EN"
	"http://www.w3.org/TR/xhtml11/DTD/xhtml11.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en">
<head>
  <title>Dynamic Forms</title>
  
  <%= stylesheet_link_tag "qe/application" %>
  
  <%= javascript_include_tag "qe/application" %>
  <%= javascript_include_tag "ckeditor/init" %>
  
  <%= csrf_meta_tag %>
</head>
<body>

<div id="status" style="display: none;">
  <div class="status_content">
    <%= image_tag('qe/status.gif', :id => "status-img" ) %> <span id="status-msg">Please Wait...</span>
  </div>
</div>

<div id="wrapper">
   <div id="header">
      <ul id="nav-main">
        <li class="active"><%= link_to 'Forms', admin_question_sheets_path %></li>
        <div class="clear"></div>
      </ul>
   </div>
   
   <div id="main">
    <%= yield %>
   </div>
</div>

</body>
</html>
