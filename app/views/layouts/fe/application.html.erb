<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.1//EN"
	"http://www.w3.org/TR/xhtml11/DTD/xhtml11.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en">
<head>
  <title><%= content_for('title') || "Dynamic Forms" %></title>
  <%= stylesheet_link_tag "print", :media => 'print' %>
  <%= stylesheet_link_tag 'base' %>

  <script type="text/javascript" charset="utf-8">
    var answer_sheet_type = '<%= @answer_sheet.class %>'
  </script>
  <%= csrf_meta_tag %>
  <%= stylesheet_link_tag 'message_block' %>
  <%= stylesheet_link_tag asset_path('fe/fe.screen.css') %>
  <script type="text/javascript" charset="utf-8">
    <%= raw Rack::TimeZone::Javascript %>
    setTimezoneCookie()
  </script>
  <%= javascript_include_tag "fe/fe.public.js" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tag %>
</head>
<body>

<div id="wrapper">
   <div id="header">
   </div>

   <div id="main">
    <%= yield %>
   </div>
</div>

<div id="footer">
  Form Engine | Created by <a href="http://www.26am.com">26AM Design Studio</a>
</div>

</body>
</html>
