<div class="submission_errors">
  <p><%= _("You still need to answer questions on the following pages:") %></p>
  <ul>
    <% @answer_sheet.pages.each do |page| %>
        <% unless page.complete?(@answer_sheet) %>
            <li class="incomplete"><%= page.label %></li>
        <% end %>
    <% end %>
  </ul>
</div>
